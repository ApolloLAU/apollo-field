{
  "expo": {
    "name": "Apollo",
    "slug": "Apollo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/png/apollo_splash.png",
    "splash": {
      "image": "./assets/png/apollo_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/png/apollo_splash.png"
    },
    "plugins": ["with-rn-bluetooth-classic"],
    "android": {
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.apollo.field"
    },
    "ios": {
      "bundleIdentifier": "com.apollo.field"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
